{
  "name": "sbotics-snippets",
  "displayName": "sBotics Snippets",
  "description": "Snippets to the sBotics simulator",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.46.0"
  },
  "categories": [
    "Snippets",
	"Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./extension.js",
  "contributes": {
    "snippets": [
      {
        "language": "csharp",
        "path": "./snippets/csharp.code-snippets"
      },
      {
        "language": "sboticsC",
        "path": "./snippets/reduc_pt_BR.code-snippets"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.46.0",
    "axios": "^0.21.1",
    "eslint": "^7.9.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.0.2",
    "vscode-test": "^1.4.0"
  },
  "dependencies": {
    "fs": "0.0.1-security"
  }
}
