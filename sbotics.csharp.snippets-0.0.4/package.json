{
    "name": "sbotics-csharp-snippets",
    "displayName": "Snippets C# sBotics",
    "description": "Snippets de C# para o simulador sBotics",
    "version": "1.0.2",
    "publisher": "Gabrieldp-dev",
    "repository": {
        "type": "git",
        "url": "https://github.com/Natalnet/sbotics-site/blob/master/images"
    },
    "icon": "favicon.png",
    "engines": {
        "vscode": "^1.47.0"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "languages": [
            {
                "id": "sboticsC",
                "extensions": [
                    ".sboticsC",
                    ".sBoticsC",
                    ".cs"
                ],
                "aliases": [
                    "sboticsC",
                    "sBoticsC",
                    "cs"
                ],
                "firstLine": "^#!\\s*/.*\\bsboticsC\\b"
            }
        ],
        "snippets": [
            {
                "languages": [
                    "sboticsC",
                    "C#"
                ],
                "path": "./snippets/sbotics-c#-snippets.code-snippets"
            }
        ]
    }
}